#wrap{width:100%;}

#gnb > li{width:20%; font-weight:bold; float:left;/* position:fixed;*/}
#gnb > li > a{display:block; 
					 padding:10px 0; 
					 text-align:center; 
					 border-top:0px solid #ddd; 
					 border-right:0px solid #ddd; 
					 border-bottom:0px solid #ddd;
					 font-size:18px;
					 color:#fff;
					 }
#gnb > li:first-child > a{border-left:0px solid #ddd;}
#gnb > li.on > a, #gnb > li > a:hover, #gnb > li > a:focus{color:#ac181e;  font-size:22px;}
#gnb > li ul{width:100%; overflow:hidden; position:absolute; top:50px; display:none;}
#gnb > li.sub_01 ul{top:60px; left:-20px;}
#gnb > li.sub_02 ul{top:60px; left:230px;}
#gnb > li.sub_03 ul{left:450px;}
#gnb > li.sub_04 ul{top:60px; left:580px;}
#gnb > li.sub_05 ul{top:60px; left:764px;}
#gnb > li ul li{float:left; margin:0 10px;  font-weight:normal;}
#gnb > li ul li a:link{font-size:16px; font-weight:bold;  color:#131d27;}
#gnb > li ul li a:hover, #gnb > li ul li a:focus{color:#ff0000; font-size:17px; font-weight:bold;}